{
  "name": "dsdeboer/google-tag-manager",
  "description": "A Google Tag Manager extension for Bolt.",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Duncan de Boer",
      "email": "duncan.d.boer@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4 | >=8.0",
    "symfony/twig-bundle": "^5.3",
    "twig/extra-bundle": "^2.12|^3.0",
    "twig/twig": "^2.12|^3.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "bolt/core": "^4.0 | ^5.0"
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "Dsdeboer\\GoogleTagManager\\": "src/"
    }
  },
  "extra": {
    "entrypoint": "Dsdeboer\\GoogleTagManager\\Extension"
  }
}
